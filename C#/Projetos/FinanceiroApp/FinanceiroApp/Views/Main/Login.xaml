<Window x:Class="FinanceiroApp.WPF.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:vm="clr-namespace:FinanceiroApp.WPF.ViewModel"
        xmlns:converter="clr-namespace:FinanceiroApp.WPF.Converter"
        xmlns:local="clr-namespace:FinanceiroApp.WPF"
        mc:Ignorable="d"
        Title="Login" Height="430" Width="400" d:Height="430" d:Width="400" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Window.Resources>
        <ResourceDictionary>
            <vm:LoginViewModel x:Key="vm"/>
        </ResourceDictionary>
    </Window.Resources>
    <Grid DataContext="{StaticResource vm}">
        <Grid.Resources>
            <converter:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter" />
            <converter:BoolToEnabledConverter x:Key="BoolToEnabledConverter" />
        </Grid.Resources>
        
    </Grid>
</Window>
